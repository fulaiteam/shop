.navbar {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
  background: url(https://jgl.oss-cn-beijing.aliyuncs.com/jiangelou_img/myCollect/%E6%A4%AD%E5%9C%86%202.png) no-repeat;
  background-size: 100% auto;
}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 230rpx;
  padding-right: 230rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  color: #333;
  font-size: 29rpx;
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navbar-action-wrap {
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: absolute;
  left: 20rpx;
  z-index: 11;
  line-height: 1;
  padding-top: 17rpx;
  padding-bottom: 17rpx;
}

.navbar-action-group {
  overflow: hidden;
}

.navbar-action_item {
  padding: 6rpx 0;
  color: #333;
}

.navbar-action-group .navbar-action_item {
  padding: 6rpx 28rpx;
}

.navbar-action-group .last {
  border-right: none;
}

.arrows {
  width: 20rpx;
  height: 20rpx;
  border-right: 1px solid #000;
  border-top: 1px solid #000;
  transform: rotate(225deg);
}