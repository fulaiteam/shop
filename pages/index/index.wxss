/* 列表 */
.clear{
  clear: both;
  overflow: hidden;
}
navigator{
  display:inline;
}
.list{
  margin-top:10px;
  background-color: #fff;
}
 
.list .list_item{
  margin-top:10px;
  padding:10px;
  height:100px;
  border-bottom:1px solid #E8E8E8;
}
.list .list_item .img{
  float:left;
  width:40%;
  height:100%;
}
.list .list_item .img image{
   width:100%;
   height:100%;
}
 
.list .list_item .info{
  width:59%;
  float:left;
  height:100px;
  position:relative;
}
.list .list_item .info .title{
  position: absolute;
  left:25rpx;
font-size: 10px;
font-family: Source Han Sans CN;
font-weight: 400;
color: #333333;
line-height: 23rpx;
}
.list .list_item .info .text{
  position: absolute;
  left:25rpx;
  bottom: 160rpx;
font-size: 9px;
font-family: Source Han Sans CN;
font-weight: 400;
color: #666666;
}
 
.list .list_item .info .price{
  color: #FF9214;
  position: absolute;
  left:25rpx;
  bottom: 0rpx;

  font-family: Source Han Sans CN;
  font-weight: 400;
  font-size:12px;

}
.list .list_item .info .original_price{
 
color: #999999;
  position: absolute;
  left:94rpx;
  bottom: 0rpx;

  text-decoration: line-through;
  font-family: Source Han Sans CN;
  font-weight: 400;
  font-size:9px;

}
.list .list_item .info .num{
  position: absolute;
  left:0rpx;
  bottom:53rpx;
  color:#747474;
  margin-left:10px;
  font-size:9px;
font-family: Source Han Sans CN;
font-weight: 400;
color: #999999;
}
.list .list_item .info .btn{
  position: absolute;
  right:-21rpx;
  bottom:53rpx;
  font-size:8px;
  width: 60px;
  height: 20px;
  text-align: center;
  line-height: 20px;
font-family: Source Han Sans CN;
font-weight: 400;
border-radius: 17px 0 0 17px;
background-color: #fda8a6;
color: #fff;
}

